{
  "name": "blackbox-universal-racing-hud",
  "version": "1.0.0",
  "description": "Universal Racing HUD System with adaptive series detection and professional telemetry interface",
  "main": "team/index.html",
  "scripts": {
    "start": "python -m http.server 8080 --directory team",
    "serve": "npx serve team -p 8080",
    "driver": "python main.py",
    "config": "python config_panel.py",
    "test": "python -m pytest tests/",
    "build": "echo 'No build step required - static files ready for deployment'"
  },
  "keywords": [
    "racing",
    "telemetry",
    "hud",
    "f1",
    "endurance",
    "nascar",
    "iracing",
    "real-time",
    "websocket",
    "voice-communication"
  ],
  "author": "PROJECT:BLACKBOX Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/blackbox-racing-hud.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/blackbox-racing-hud/issues"
  },
  "homepage": "https://github.com/your-org/blackbox-racing-hud#readme",
  "engines": {
    "node": ">=14.0.0",
    "python": ">=3.8.0"
  },
  "dependencies": {
    "serve": "^14.0.0"
  },
  "devDependencies": {
    "eslint": "^8.0.0",
    "prettier": "^2.0.0"
  },
  "files": [
    "team/",
    "telemetry/",
    "voice/",
    "network/",
    "config/",
    "ui/",
    "main.py",
    "requirements.txt",
    "README.md",
    "DEPLOYMENT.md"
  ],
  "directories": {
    "lib": "./",
    "doc": "./docs",
    "test": "./tests"
  }
}
